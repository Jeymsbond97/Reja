<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/main.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <title>Portfolio</title>
</head>
<body>
      <div class="frame">
        <div class="main_frame">

            <!-- Profile section -->

            <div class="left_frame">
                <div class="left_1">
                    <div class="left_1-1">
                        <img 
                            src="images/Profile.png" alt=""
                            width="156px"
                            height="142px"
                        />
                    </div>
                    <div class="left_1-2">
                        <%=user.name%>
                    </div>
                    <div class="left_1-3">
                        <%=user.profession%>
                    </div>
                    <div class="left_icons">
                        <a class="link_href" href="">
                            <div class="icon">
                                <img src="icon/Icons-facebook.png"  class="icon_1">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img src="icon/Icons-instagram.png" class="icon_1">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img src="icon/Icons-linkedin.png" class="icon_1">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img src="icon/Icons-twitter.png" class="icon_1">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img src="icon/Icons-youtube.png" class="icon_1">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img src="icon/Icons-dribbble.png" class="icon_1">
                            </div>
                        </a>
                    </div>
                </div>
                <div class="marginer" style="margin-top: 60px;"></div>
                     
                <!-- About section -->

                <div class="left_2">
                    <div class="left_2-1">
                        <div class="left_2-info">Age:</div>
                        <div class="left_2-data"><%=user.age%></div>
                    </div>
                    <div class="left_2-1">
                        <div class="left_2-info">Residence:</div>
                        <div class="left_2-data"><%=user.residence%></div>
                    </div>
                    <div class="left_2-1">
                        <div class="left_2-info">Freelance:</div>
                        <div class="left_2-info-hand" style="color: rgb(126, 185, 66) ;">Available</div>
                    </div>
                    <div class="left_2-1">
                        <div class="left_2-info">Address:</div>
                        <div class="left_2-data"><%=user.adress%></div>
                    </div>
                </div>

                <!-- End About section -->

                <div class="marginer"></div>

                <!-- Languages -->
                <div class="left_3">
                    <div class="languages">
                        Languages
                    </div>
                    <div class="languages_bar">
                        <% user.languages.forEach(function(language) { %>
                            <div>
                                <div class="languages_one"><%= language.name %></div>
                            </div>
                            <div class="progress">
                                <div class="progress-bar bg-warning" 
                                     role="progressbar" 
                                     style="width: <%= language.progress %>%" 
                                     aria-valuenow="<%= language.progress %>" 
                                     aria-valuemin="0" 
                                     aria-valuemax="100">
                                    <%= language.progress %>%
                                </div>
                            </div>
                        <% }); %>
                    </div>   
                </div>
                <!-- End Language -->
                <div class="marginer"></div>
                <!-- Skills -->
                <div class="left_4">
                    <div class="skills">
                        Skills
                    </div>
                    <div class="skill">
                        <% user.skills.forEach(function(skill) { %>
                            <div>
                                <div class="skill-one"><%= skill.name %></div>
                            </div>
                            <div class="progress">
                                <div class="progress-bar bg-warning" 
                                     role="progressbar" 
                                     style="width: <%= skill.progress %>%" 
                                     aria-valuenow="<%= skill.progress %>" 
                                     aria-valuemin="0" 
                                     aria-valuemax="100">
                                    <%= skill.progress %>%
                                </div>
                            </div>
                        <% }); %>
                    </div>
                </div>
                <!-- End Skills -->
                <div class="marginer"></div>
                <!-- Extra Skills -->
                 
                <div class="left_5">
                    <div class="extra_skills">
                        Extra Skills
                    </div>
                    <div class="extra_skills-items">
                        <% user.extra_skills.forEach(function(skill) { %>
                            <div class="extra_skill">
                                <img src="icon/icons.png" class="skill-icon">
                                <span class="extra_skill-item"><%= skill %></span>
                            </div>
                        <% }); %>
                    </div>

                </div>
                <!-- End Extra Skills -->
                <div class="marginer"></div>
            </div>
            <div class="middle_frame">
                <div class="middle_top">
                    <div class="middle_top-div">
                        <div class="middle_top-info">
                            <div class="round_1">
                                <img src="icon/1.svg" alt="">
                            </div>
                            <div class="round_2">
                                <img src="icon/2.svg" alt="">
                            </div>
                            <div class="round_3">
                                <img src="icon/3.svg" alt="">
                            </div>
                            <div class="round_4">
                                <img src="icon/4.svg" alt="">
                            </div>
                            <div class="round_5">
                                <img src="icon/5.svg" alt="">
                            </div>
                            <div class="middle_top-main">
                                <div class="main_info">
                                    <%=user.profession%> in <%=user.work_place%>
                                </div>
                            </div>
                            <div class="middle_top-text">
                                <p class="middle_dec">
                                    Lorem ipsum dolor sit amet, consectetur adipiscing elit. Et, volutpat feugiat placerat lobortis. Natoque rutrum semper sed suspendisse nunc lectus.
                                </p>
                            </div>
                            <div class="middle_top-btn">
                                <button class="btn1">
                                   <p>HIRE ME</p>
                                   <img src=" btn_icon/btn1.png" alt="">

                                </button>
                            </div>
                        </div>
                        <div class="middle_top-img">
                            <img src="images/img.png" alt="">
                        </div>
                    </div>
                </div>
                <div class="middle_bot">
                    <div class="main_service-card">
                        <div class="my_services">My Service</div>
                     <div class="my_services-info">Amet minim mollit non deserunt ullamco est sit aliqua dolor do amet sint. Velit officia consequat duis enim velit mollit. lorem ipsum</div>
                    </div> 
                    <div class="service_card">
                        <div class="card">
                            <div class="card_icon">
                                <img src="icon/icon5.png" alt="">
                            </div>
                            <div class="card_title">
                                UI/UX Design
                            </div>
                            <p class="card_text">Mobile app, website design</p>
                        </div>
                        <div class="card">
                            <div class="card_icon">
                                <img src="icon/icon4.png" alt="">
                            </div>
                            <div class="card_title">Wev Development</div>
                            <p class="card_text">Blog, E-Commerce</p>
                        </div>
                        <div class="card">
                            <div class="card_icon">
                                <img src="icon/icon3.png" alt="">
                            </div>
                            <div class="card_title">
                                Sound Design
                            </div>
                            <p class="card_text">Voice Over, Beat Making</p>
                        </div>
                        <div class="card">
                            <div class="card_icon">
                                <img src="icon/icon1.png" alt="">
                            </div>
                            <div class="card_title">
                                Game Design
                            </div>
                            <p class="card_text">Character Design, Props & Object</p>
                        </div>
                        <div class="card">
                            <div class="card_icon">
                                <img src="icon/icon5.png" alt="">
                            </div>
                            <div class="card_title">
                                Photography
                            </div>
                            <p class="card_text">portrait, product photography</p>
                        </div>
                        <div class="card">
                            <div class="card_title" style="margin-bottom: 15px;">Advertising</div>
                            <div class="card_text" style="margin-bottom: 10px;">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Vitae nulla diam in ac dictum a urna viverra morbi.</div>
                            <div class="service_order">
                                <div class="order_item">
                                    <a href="#">ORDER NOW</a>
                                </div>
                                <div class="order_icon">
                                    <img src="icon/order.png" alt="">
                                </div>
                            </div>
                        </div>
                    </div>
                
                </div>
            </div>
            <div class="right_frame">
                <div class="top_icon">
                    <img src="icon/Vector0.png" alt="">
                </div>
                <div class="icons_list">
                    <div class="icon1 main_icon">
                        <img src="icon/Vector1.png" alt="">
                    </div>
                    <div class="icon">
                        <img src="icon/Vector2.png" alt="">
                    </div>
                    <div class="icon">
                        <img src="icon/Vector3.png" alt="">
                    </div>
                    <div class="icon">
                        <img src="icon/Vector4.png" alt="">
                    </div>
                    <div class="icon">
                        <img src="icon/Vector5.png" alt="">
                    </div>
                    <div class="icon">
                        <img src="icon/Vector6.png" alt="">
                    </div>
                </div>
            </div>

            <!-- djdjfdfhdfsdfdkikdfdkf -->
        </div>
      </div>
</body>
</html>


